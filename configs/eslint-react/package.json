{
  "name": "@elkevinwolf/eslint-config-react",
  "version": "1.0.0",
  "description": "Kevin Wolf's opinionated shareable ESLint config for React projects",
  "license": "MIT",
  "author": {
    "name": "Kevin Wolf",
    "email": "hi@kevinwolf.dev",
    "url": "https://kevinwolf.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elkevinwolf/devtools",
    "directory": "configs/eslint-react"
  },
  "bugs": {
    "url": "https://github.com/elkevinwolf/devtools/issues",
    "email": "hi@kevinwolf.dev"
  },
  "keywords": [
    "eslint",
    "eslint config",
    "eslint react config",
    "shareable eslint config",
    "elkevinwolf"
  ],
  "main": ".eslintrc.json",
  "files": [
    ".eslintrc.json"
  ],
  "scripts": {
    "build": "echo No build script for @elkevinwolf/eslint-config-react",
    "lint": "prettier --ignore-path .eslintignore --check .",
    "test": "echo No test script for @elkevinwolf/eslint-config-react",
    "validate": "npm run build && npm run lint && npm run test",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@elkevinwolf/eslint-config-base": "1.0.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0"
  },
  "peerDependencies": {
    "eslint": ">=7.25.0",
    "prettier": ">=2.2.1",
    "typescript": ">=4.2.4"
  },
  "devDependencies": {
    "@elkevinwolf/prettier-config": "1.0.0",
    "eslint": "7.25.0",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "typescript": "4.2.4"
  },
  "prettier": "@elkevinwolf/prettier-config",
  "lint-staged": {
    "*.{json,md}": "prettier --write"
  }
}
